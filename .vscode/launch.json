{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "example_dynamic_wasm",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/builds/x64/example_dynamic_wasm",
            "args": [
                "${workspaceFolder}/test.jpeg",
                "${workspaceFolder}/test_out.jpeg",
                "${workspaceFolder}/builds/x64/wasm_build/.libs/libjpeg_wasm.so"
            ],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/builds",
            "environment": [],
            "externalConsole": true,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}